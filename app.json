{
  "expo": {
    "name": "Habit Stopper",
    "slug": "habit-stopper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F0F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.indicreative.habitstopper"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F0F0F"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.indicreative.habitstopper"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#4ADE80"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "02e72287-fb8d-47db-97ff-080398afa14a"
      }
    },
    "owner": "indicreative"
  }
}
